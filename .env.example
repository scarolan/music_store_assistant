# OpenAI API Key (required for supervisor and support rep)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Google Gemini API Key (optional - for music expert)
GEMINI_API_KEY=your-gemini-api-key-here

# Music Expert Model Selection
# Options: "gemini" or "openai" (default: openai)
MUSIC_EXPERT_MODEL=gemini

# LangSmith Configuration (required for tracing)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=lsv2_your-langsmith-api-key-here
LANGCHAIN_PROJECT=music-store-assistant

# Optional: LangSmith endpoint (defaults to cloud)
# LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
